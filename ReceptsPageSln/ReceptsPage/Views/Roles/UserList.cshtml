@model IEnumerable<ReceptsPage.ModelIdentity.AppUser>

<h2>Օգտատերերի ցուցակ</h2>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Մուտքի իրավունքները</a>
            </td>
        </tr>
    }
</table>